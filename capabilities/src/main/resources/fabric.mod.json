{
  "schemaVersion": 1,
  "id": "porting_lib_capabilities",
  "version": "${version}",
  "name": "Porting Lib Capabilities",
  "description": "Adds Forge's Capability system for developers.",
  "mixins": [
    "porting_lib_capabilities.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.capabilities.PortingLibCapabilities"
    ]
  },
  "custom": {
