  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_1799": ["xyz/bluspring/forgecapabilities/extensions/capabilities/CapabilityProviderExtension", "xyz/bluspring/forgecapabilities/extensions/capabilities/ItemStackCapabilityProviderImpl"],
      "net/minecraft/class_1297": ["xyz/bluspring/forgecapabilities/extensions/capabilities/CapabilityProviderExtension", "xyz/bluspring/forgecapabilities/extensions/capabilities/EntityCapabilityProviderImpl"],
      "net/minecraft/class_2586": ["xyz/bluspring/forgecapabilities/extensions/capabilities/CapabilityProviderExtension", "xyz/bluspring/forgecapabilities/extensions/capabilities/BlockEntityCapabilityProviderImpl"],
      "net/minecraft/class_1937": ["xyz/bluspring/forgecapabilities/extensions/capabilities/CapabilityProviderExtension", "xyz/bluspring/forgecapabilities/extensions/capabilities/LevelCapabilityProviderImpl"],
      "net/minecraft/class_3218": ["xyz/bluspring/forgecapabilities/extensions/capabilities/InitializableCapabilityExtension"],
      "net/minecraft/class_1792": ["xyz/bluspring/forgecapabilities/extensions/capabilities/ItemCapabilityExtension"]
    }
  }
}
