      "net/minecraft/class_1297": ["io/github/fabricators_of_create/porting_lib/extensions/capabilities/CapabilityProviderExtension", "io/github/fabricators_of_create/porting_lib/extensions/capabilities/EntityCapabilityProviderImpl"],
      "net/minecraft/class_2586": ["io/github/fabricators_of_create/porting_lib/extensions/capabilities/CapabilityProviderExtension", "io/github/fabricators_of_create/porting_lib/extensions/capabilities/BlockEntityCapabilityProviderImpl"],
      "net/minecraft/class_1937": ["io/github/fabricators_of_create/porting_lib/extensions/capabilities/CapabilityProviderExtension", "io/github/fabricators_of_create/porting_lib/extensions/capabilities/LevelCapabilityProviderImpl"],
      "net/minecraft/class_3218": ["io/github/fabricators_of_create/porting_lib/extensions/capabilities/InitializableCapabilityExtension"],
      "net/minecraft/class_1792": ["io/github/fabricators_of_create/porting_lib/extensions/capabilities/ItemCapabilityExtension"]
    }
  }
}
