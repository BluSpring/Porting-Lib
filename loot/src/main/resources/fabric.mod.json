{
  "schemaVersion": 1,
  "id": "porting_lib_loot",
  "version": "${version}",
  "name": "Porting Lib Loot",
  "description": "Allows mods to modify loot more easily.",
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.loot.PortingLibLoot"
    ]
  },
  "mixins": [
    "porting_lib_loot.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_47": ["io/github/fabricators_of_create/porting_lib/loot/extensions/LootContextExtensions"],
      "net/minecraft/class_52": ["io/github/fabricators_of_create/porting_lib/loot/extensions/LootTableExtensions"]
    }
  }
}
